{
    "ListenURL": ":1234",
    "URLPrefix": "/staff",
    "Logger": {
        "UseFile": false,
        "FileName": "staff-manager.log",
        "LogLevel": "DEBUG"
    },
    "OriginHosts": ["http://localhost:3000"],

    "AWSSecretName": "heroku/secrets",
    "AWSRegion": "eu-central-1",
    "BucketName": "staff-users",

    "ElasticSearch": {
        "URLs": ["http://127.0.0.1:9200"],
        "MaxIdleConns": 50,
        "IdleConnTimeoutInSec": 30,
        "ClientTimeoutInSec": 60
    }
}